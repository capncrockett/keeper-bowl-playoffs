{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": false,
    "isolatedModules": false,
    "noEmit": false,
    "outDir": "./node_modules/.tmp/ts-jest",
    "types": ["node", "jest"]
  }
}
